                <item row="3" column="5">
                 <widget class="QComboBox" name="ComboTipoExpediente">
                  <property name="cursor">
                   <cursorShape>PointingHandCursor</cursorShape>
                  </property>
                  <property name="styleSheet">
                   <string notr="true">QComboBox {
    background-color: #f5f5f5; /* Mismo fondo gris claro que el QLineEdit */
    border: none; /* Sin bordes generales */
    border-bottom: 1px solid #CCCCCC; /* Borde inferior suave */
    border-radius:2px; /* Bordes redondeados suaves */
    padding: 5px 5px; /* Espaciado interno */
    font-size: 18px; /* Tamaño de fuente más elegante */
    color: #333333; /* Texto en gris oscuro */
    transition: all 0.3s ease; /* Transición suave */
	
}

/* Efecto al pasar el mouse */
QComboBox:hover {
    border-bottom: 2px solid #16344D; /* Engrosa el borde inferior al pasar el mouse */
    background-color: #f0f0f0; /* Cambio sutil de fondo */
}

/* Cuando el ComboBox está en foco */
QComboBox:focus {
    background-color: #e0e0e0; /* Gris más oscuro cuando está activo */
    border-bottom: 2px solid #16344D; /* Mantiene el borde inferior */
    box-shadow: 0 0 8px rgba(22, 52, 77, 0.3); /* Sombra más suave y elegante */
    transition: box-shadow 0.3s ease; /* Transición suave para la sombra */
}

/* Personalización del menú desplegable */
QComboBox QAbstractItemView {
    background-color: #f5f5f5; /* Fondo gris claro */
    color: #333333; /* Texto gris oscuro */
    border-radius: 5px; /* Bordes redondeados suaves */
    selection-background-color: #16344D; /* Fondo de selección azul elegante */
    selection-color: white; /* Texto blanco al seleccionar */
    padding: 5px 0; /* Espaciado interno */
    min-width: 150px; /* Ancho mínimo para una mejor apariencia */
}

/* Cambio de color cuando se pasa el mouse por una opción */
QComboBox QAbstractItemView::item:hover {
    background-color: #16344D; /* Fondo oscuro al pasar el mouse */
    color: white; /* Texto blanco */
}

/* Cambio de color cuando una opción está seleccionada */
QComboBox QAbstractItemView::item:selected {
    background-color: #16344D; /* Mismo fondo de selección */
    color: white; /* Texto en blanco */
}
</string>
                  </property>
                 </widget>
